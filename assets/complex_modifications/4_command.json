{
  "title":"4.Command",
  "rules":[
    {
      "description":"4. Eisu + F,G to Find,Replace",
      "manipulators":[
        {
          "type":"basic",
          "from":{
            "key_code":"f",
            "modifiers":{
              "mandatory":["right_control"],
              "optional":["caps_lock"]
            }
          },
          "to":[
            {
              "key_code":"f",
              "modifiers":["command"]
            }
          ]
        },
        {
          "type":"basic",
          "from":{
            "key_code":"g",
            "modifiers":{
              "mandatory":["right_control"],
              "optional":["caps_lock"]
            }
          },
          "to":[
            {
              "key_code":"f",
              "modifiers":["option","command"]
            }
          ]
        }
      ]
    },
