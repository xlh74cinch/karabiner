{
  "title":"8.shell",
  "rules":[
    {
      "description":"8. Eisu + spacebar to open Alfred app",
      "manipulators":[
        {
          "type":"basic",
          "from":{
            "key_code":"spacebar",
            "modifiers":{
              "mandatory":["right_control"],
              "optional":["caps_lock"]
            }
          },
          "to":[
            {
              "shell_command": "open -a 'Alfred 4.app'"
            }
          ]
        }
      ]
    }
  ]
}