{
  "title":"Search",
  "rules":[
    {
      "description":"Ctrl + F/G to Find / Replace",
      "manipulators":[
        {
          "type":"basic",
          "from":{
            "key_code":"f",
            "modifiers":{
              "mandatory":["control"],
              "optional":["caps_lock"]
            }
          },
          "to":[
            {
              "key_code":"f",
              "modifiers":["command"]
            }
          ]
        },
        {
          "type":"basic",
          "from":{
            "key_code":"g",
            "modifiers":{
              "mandatory":["control"],
              "optional":["caps_lock"]
            }
          },
          "to":[
            {
              "key_code":"f",
              "modifiers":["option","command"]
            }
          ]
        }
      ]
    }
  ]
}
